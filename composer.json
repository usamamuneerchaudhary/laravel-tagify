{
    "name": "usamamuneerchaudhary/laravel-tagify",
    "description": "An Eloquent Tagging package for Laravel 10",
    "keywords": [
        "usamamuneerchaudhary",
        "laravel-tagging",
        "taggables",
        "tagify",
        "tagging",
        "laravel"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Usama Muneer",
            "email": "hello@usamamuneer.me",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Usamamuneerchaudhary\\LaravelTagify\\": "src"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/database": ">=v10.1.4",
        "illuminate/support": ">=v10.1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^v8.0.4"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/stubs/LessonStub.php",
            "tests/stubs/TagStub.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
